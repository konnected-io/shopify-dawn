    "k_section_text_feature_GinA8b": {
      "type": "k-section-text-feature",
      "blocks": {
        "item_mXhh9w": {
          "type": "item",
          "settings": {
            "title": "Updated processor",
            "content": "<p>Now built on the ESP32-S3-MINI, a more powerful and capable IoT chip that adds Bluetooth, BLE, and more memory and processing power to ensure future compatibility with evolving IoT standards like <a href=\"https://csa-iot.org/all-solutions/matter/\" target=\"_blank\" title=\"https://csa-iot.org/all-solutions/matter/\">Matter.</a></p>"
          }
        },
        "item_GxaYQq": {
          "type": "item",
          "settings": {
            "title": "Multi-Tonal alarm",
            "content": "<p>Adds more pleasant and customizable warning tones thanks to an upgrade to the built-in sounder on the device.</p>"
          }
        },
        "item_ciXJmz": {
          "type": "item",
          "settings": {
            "title": "Made for ESPHome",
            "content": "<p><a href=\"https://esphome.io/guides/made_for_esphome.html\" target=\"_blank\" title=\"https://esphome.io/guides/made_for_esphome.html\">ESPHome-based firmware</a> pre-installed</p>"
          }
        },
        "item_LUAjmL": {
          "type": "item",
          "settings": {
            "title": "Open source",
            "content": "<p>designed with an emphasis on<em>choice</em>, <em>privacy</em>, and <em>local </em>control</p>"
          }
        }
      },
      "block_order": [
