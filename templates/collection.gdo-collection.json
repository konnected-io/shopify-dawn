d>\n          <td class=\"yes\">✔️</td>\n          <td class=\"no\">❌</td>\n          <td class=\"yes\">✔️</td>\n          <td class=\"yes\">✔️</td>\n        </tr>\n        <tr>\n          <td>Free integrations with 3rd-party apps</td>\n          <td class=\"yes\">✔️</td>\n          <td class=\"no\">❌</td>\n          <td class=\"yes\">✔️</td>\n          <td class=\"no\">❌</td>\n          <td class=\"no\">❌</td>\n        </tr>\n        <tr>\n          <td>Open source</td>\n          <td class=\"yes\">✔️</td>\n          <td class=\"no\">❌</td>\n          <td class=\"no\">❌</td>\n          <td class=\"no\">❌</td>\n          <td class=\"yes\">✔️</td>\n        </tr>\n        <tr>\n          <td>Partial open/close</td>\n          <td class=\"yes\">✔️</td>\n          <td class=\"no\">❌</td>\n          <td class=\"yes\">✔️</td>\n          <td class=\"no\">❌</td>\n          <td class=\"yes\">✔️</td>\n        </tr>\n        <tr>\n          <td>Controls light and remote lock (GDO blaQ only)</td>\n          <td class=\"yes\">✔️</td>\n          <td class=\"no\">❌</td>\n          <td class=\"no\">❌</td>\n          <td class=\"no\">❌</td>\n          <td class=\"no\">❌</td>\n        </tr>\n        <tr>\n          <td>Designed and assembled in USA</td>\n          <td class=\"yes\">✔️</td>\n          <td class=\"no\">❌</td>\n          <td class=\"no\">❌</td>\n          <td class=\"no\">❌</td>\n          <td class=\"no\">❌</td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</section>",
